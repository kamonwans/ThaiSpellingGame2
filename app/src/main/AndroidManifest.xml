        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.RandomUserActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.GameEasyActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.GameHardActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.GameMediumActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.LearnActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.SummaryAllActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.MenuActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.TestActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.SummaryCategoryActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.ProfileActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.ContactActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.SettingActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.LearnRoyalActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".activity.LearnUseEverdayActivity" />
        <activity android:name=".activity.LearnTransliterateActivit"></activity>
    </application>

</manifest>