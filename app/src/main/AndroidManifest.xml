<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kamonwan.thaispellinggame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RandomUserActivity"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name=".GameEasyActivity"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name=".GameHardActivity"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name=".GameMediumActivity"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name=".LearnActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".SummaryAllActivity"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name=".MenuActivity"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name=".TestActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".SummaryCategoryActivity"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name=".ProfileActivity"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name=".ContactActivity"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name=".SettingActivity"
            android:screenOrientation="landscape"></activity>
    </application>

</manifest>